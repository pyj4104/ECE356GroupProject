<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>SchemaServlet</servlet-name>
        <servlet-class>ece356.SchemaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>ece356.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DocViewProfServlet</servlet-name>
        <servlet-class>ece356.DocViewProfServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FriendRequestsServlet</servlet-name>
        <servlet-class>ece356.FriendRequestsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientSearchServlet</servlet-name>
        <servlet-class>ece356.PatientSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DocViewProfServlet</servlet-name>
        <url-pattern>/DocViewProfServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FriendRequestsServlet</servlet-name>
        <url-pattern>/FriendRequestsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientSearchServlet</servlet-name>
        <url-pattern>/PatientSearchServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
